<Window x:Class="DataAnalysis_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid Margin="0,0,0,0" Background="#FFEDEDED">
        <Rectangle Fill="#FFFAFAFA" HorizontalAlignment="Left" Height="261" Margin="10,10,0,0" Stroke="#FFC2C2C2" VerticalAlignment="Top" Width="498"/>
        <Button Content="Generate" HorizontalAlignment="Left" Margin="345,291,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>
        <Button Content="Browse" HorizontalAlignment="Left" Margin="254,291,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="433,291,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_2"/>
        <TextBox x:Name="filePath" HorizontalAlignment="Left" Height="23" Margin="10,291,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="224" TextChanged="TextBox_TextChanged"/>
        <TextBlock x:Name="ImportMessage" HorizontalAlignment="Left" Margin="26,19,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="17" Width="471"/>

    </Grid>
</Window>
